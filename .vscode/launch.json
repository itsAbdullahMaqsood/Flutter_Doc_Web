{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Flutter Web (Chrome, localhost:3000)",
      "request": "launch",
      "type": "dart",
      "program": "lib/main.dart",
      "args": [
        "-d",
        "chrome",
        "--web-hostname",
        "localhost",
        "--web-port",
        "3000",
        "--web-enable-expression-evaluation"
      ]
    },
    {
      "name": "web_google_docs",
      "request": "launch",
      "type": "dart"
    },
    {
      "name": "web_google_docs (profile mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "profile"
    },
    {
      "name": "web_google_docs (release mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "release"
    }
  ]
}
